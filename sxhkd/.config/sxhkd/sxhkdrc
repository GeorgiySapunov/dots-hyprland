# Basic binds
super + Return
				$TERMINAL
# super + shift + Return

super + x
                sd
super + shift + x
                ibus engine xkb:us::eng; gpg-connect-agent --no-autostart reloadagent /bye; betterlockscreen -l
super + n
				$TERMINAL -e nvim ~/git/notes/README.md
super + shift + n
				$TERMINAL -e newsboat; pkill -RTMIN+6 dwmblocks
super + d
				ibus engine xkb:us::eng && dmenu_run
super + shift + d
				ibus engine xkb:us::eng && passmenu
super + r
				ibus engine xkb:us::eng && $TERMINAL -e lf
super + shift + r
				$TERMINAL -e htop
super + e
				$TERMINAL -e neomutt ; pkill -RTMIN+12 dwmblocks; rmdir ~/.abook
super + shift + e
				$TERMINAL -e abook
super + m
				$TERMINAL -e ncmpcpp
# super + shift + m
#                 emacsclient -c -a 'emacs'
super + c
				$TERMINAL -e calcurse
super + g
                telegram-desktop
super + shift + g
                tor-browser
super + shift + a
				$TERMINAL -e alsamixer || pulsemixer; pkill -RTMIN+10 $STATUSBAR
super + shift + c
				mpv --no-osc --no-input-default-bindings --input-conf=/dev/null --title='mpvfloat' /dev/video0
super + w
				$BROWSER
super + shift + w
				$TERMINAL -e sudo nmtui
super + ctrl + w
				qutebrowser

# Et cetera...
super + grave
				dmenuunicode
super + Scroll_Lock
				killall screenkey || screenkey
super + Insert
				showclip
super + BackSpace
				ibus engine xkb:us::eng && sysact
XF86Launch1
				xset dpms force off
XF86TouchpadToggle
				(synclient | grep "TouchpadOff.*1" && synclient TouchpadOff=0) || synclient TouchpadOff=1
XF86Touchpad{Off,On}
				synclient TouchpadOff={1,0}
XF86MonBrightnessDown
				xbacklight -dec 15
XF86MonBrightnessUp
				xbacklight -inc 15

# Recording
# Take screenshot
Print
				maim pic-full-$(date '+%y%m%d-%H%M-%S').png
# Pick screenshot type
shift + Print
				maimpick
# Print: Record audio or video; Del: kill recording
super + Print
				dmenurecord
super + Delete
				dmenurecord kill

# Pause audio
super + p
				mpc toggle
super + shift + p
				mpc pause; pauseallmpv
# Changes to next/previous tracks
super + {comma,period}
				mpc {prev,next}
# Restart track
super + shift + less
				mpc seek 0%
super + shift + greater
				mpc repeat
# Seek foward in song
super + {_,shift +} bracketright
				mpc seek +{10,60}
# Seek backward in song
super + {_,shift +} bracketleft
				mpc seek -{10,60}
# Increase volume
super + {equal,plus}
                wpctl set-volume @DEFAULT_AUDIO_SINK@ {5,15}%+; kill -44 $(pidof dwmblocks)
				# pamixer --allow-boost -i {5,15}; kill -44 $(pidof dwmblocks)
# Decrease volume
super {_,shift +} + minus
                wpctl set-volume @DEFAULT_AUDIO_SINK@ {5,15}%-; kill -44 $(pidof dwmblocks)
				# pamixer --allow-boost -d {5,15}; kill -44 $(pidof dwmblocks)
super + shift + m
                wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle; kill -44 $(pidof dwmblocks)
				# pamixer -t; kill -44 $(pidof dwmblocks)

# Audiokeys
XF86AudioMute
                wpctl set-mute @DEFAULT_AUDIO_SINK@ toggle; kill -44 $(pidof dwmblocks)
				# pamixer -t; kill -44 $(pidof dwmblocks)
XF86Audio{Raise,Lower}Volume
                wpctl set-volume @DEFAULT_AUDIO_SINK@ 3%{+,-}; kill -44 $(pidof dwmblocks)
				# pamixer --allow-boost -{i,d} 3; kill -44 $(pidof dwmblocks)
XF86Audio{Next,Prev}
				mpc {next,prev}
XF86Audio{Pause,Play,Stop}
				mpc {pause,play,stop}
XF86Audio{Rewind,Forward}
				mpc seek {-,+}10
XF86AudioRecord
				dmenurecord
XF86AudioMedia
				$TERMINAL -e ncmpcpp

# XF86PowerOff
				# ibus engine xkb:us::eng && sysact
XF86Calculator
				$TERMINAL -e bc -l
XF86Sleep
				loginctl suspend -i
XF86WWW
				$BROWSER
XF86DOS
				$TERMINAL
XF86ScreenSaver
				ibus engine xkb:us::eng && betterlockscreen -l & xset dpms force off; mpc pause; pauseallmpv
XF86TaskPane
				$TERMINAL -e htop
XF86Mail
				$TERMINAL -e neomutt ; pkill -RTMIN+12 dwmblocks
XF86MyComputer
				$TERMINAL -e lf /

# Change display
super + F1
				groff -mom /usr/local/share/dwm/larbs.mom -Tpdf | zathura -
super + F2
				tutorialvids
super + F3
				displayselect
super + F4
				$TERMINAL -e pulsemixer; kill -44 $(pidof dwmblocks)
# Start torrent daemon/open interface
super + F6
				torwrap
# toggle torrent daemon
super + F7
				td-toggle
# sync email
super + F8
				mw -Y
# Mount a USB drive or Android device
super + {F9,F10}
				{dmenumount,dmenuumount}
super + F11
				mpv --no-cache --no-osc --no-input-default-bindings --profile=low-latency --input-conf=/dev/null --title=webcam $(ls /dev/video[0,2,4,6,8] | tail -n 1)
super + F12
				remaps
